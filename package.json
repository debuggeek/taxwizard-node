{
  "name": "fivestone_node",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "if-env NODE_ENV=prod && npm run start:prod || if-env NODE_ENV=aws && npm run start:aws || npm run start:dev",
    "start:prod": "node ./bin/www",
    "start:dev": "supervisor --inspect ./bin/www",
    "start:aws": "NODE_ENV=prod && node ./bin/www-aws",
    "start:local": "node $NODE_DEBUG_OPTION ./bin/www"
  },
  "dependencies": {
    "body-parser": "^1.16.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "express": "~4.13.1",
    "hbs": "^4.0.1",
    "jszip": "^3.1.5",
    "morgan": "^1.8.1",
    "mysql": "^2.5.4",
    "mysql2": "^1.2.0",
    "sequelize": "^3.30.4",
    "serve-favicon": "^2.4.1"
  },
  "devDependencies": {
    "if-env": "^1.0.0"
  }
}
